   # Google AI API Key (Required)
   # Get your API key from: https://makersuite.google.com/app/apikey
   # NOTE* Remember to update line 14 in gemini_api.py if using a model that is not 2.0-flash
   GOOGLE_AI_API_KEY=your-gemini-api-key-here


   # List of users whose meetings should be processed (Required)
   # Comma-separated list of email addresses
   USERS_TO_PROCESS=user1@example.com,user2@example.com

   # Folder mapping for meeting categories (Required)
   # Replace the folder IDs with your actual Google Drive folder IDs
   FOLDER_MAPPING={
       "Team Meeting": "1YourTeamMeetingFolderId",
       "Client Meeting": "1YourClientMeetingFolderId",
       "Project Update": "1YourProjectUpdateFolderId",
       "Sales Call": "1YourSalesCallFolderId",
       "HR & Recruitment": "1YourHRFolderId",
       "Other": "1YourOtherMeetingFolderId"
   }

   # Optional: Customize the time window for processing meetings (in hours)
   # Default: 2 hours
   PROCESSING_WINDOW_HOURS=2

   # Optional: Set the minimum confidence score for categorization (0.0 to 1.0)
   # Default: 0.7
   MIN_CONFIDENCE_SCORE=0.7